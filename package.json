    "update-deps": "pnpm update -r --interactive --latest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/node": "^20.17.47",
    "eslint": "^9.0.0",
    "eslint-config-turbo": "^2.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}